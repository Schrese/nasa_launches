{
  "name": "Next Space Shuttle Launch",
  "version": "1.0",
  "description": "Extension that tells you when the next NASA space shuttle launch is.",
  "permissions": [ "activeTab", "declarativeContent", "storage" ],

  "browser_action": {
    "page": "displaying.html",
    "default_popup": "displaying.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },

    "manifest_version": 2
  }
